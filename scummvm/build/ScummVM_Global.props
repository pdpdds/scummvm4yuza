<?xml version="1.0" encoding="utf-8"?>
<Project DefaultTargets="Build" ToolsVersion="16.0" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
	<PropertyGroup>
		<_PropertySheetDisplayName>ScummVM_Global</_PropertySheetDisplayName>
		<ExecutablePath>$(SCUMMVM_LIBS)\bin;$(SCUMMVM_LIBS)\bin\x86;$(ExecutablePath)</ExecutablePath>
		<LibraryPath>$(SCUMMVM_LIBS)\lib\x86;$(SCUMMVM_LIBS)\lib\x86\$(Configuration);$(LibraryPath)</LibraryPath>
		<IncludePath>$(SCUMMVM_LIBS)\include;$(SCUMMVM_LIBS)\include\SDL2;$(IncludePath)</IncludePath>
		<OutDir>$(Configuration)32\</OutDir>
		<IntDir>$(Configuration)32\$(ProjectName)\</IntDir>
	</PropertyGroup>
	<ItemDefinitionGroup>
		<ClCompile>
			<DisableLanguageExtensions>true</DisableLanguageExtensions>
			<DisableSpecificWarnings>4068;4100;4103;4127;4244;4250;4310;4345;4351;4512;4702;4706;4800;4996;6204;6211;6385;6386;4267;4577;;%(DisableSpecificWarnings)</DisableSpecificWarnings>
			<AdditionalIncludeDirectories>.;D:\github\yuzaos\sample\emulator\scummvm;D:\github\yuzaos\sample\emulator\scummvm\engines;%(AdditionalIncludeDirectories)</AdditionalIncludeDirectories>
			<PreprocessorDefinitions>USE_ZLIB;USE_MAD;USE_OGG;USE_VORBIS;USE_FLAC;USE_PNG;USE_FREETYPE2;USE_JPEG;USE_FLUIDSYNTH;USE_SCALERS;USE_HQ_SCALERS;USE_RGB_COLOR;USE_HIGHRES;USE_MT32EMU;USE_LUA;USE_NASM;USE_OPENGL;USE_GLES;USE_TASKBAR;USE_CLOUD;USE_TRANSLATION;USE_SYSDIALOGS;USE_DETECTLANG;ENABLE_HUGO;WIN32;SDL_BACKEND;USE_SDL2;%(PreprocessorDefinitions)</PreprocessorDefinitions>
			<ExceptionHandling></ExceptionHandling>
			<RuntimeTypeInfo>true</RuntimeTypeInfo>
			<WarningLevel>Level4</WarningLevel>
			<TreatWarningAsError>false</TreatWarningAsError>
			<CompileAs>Default</CompileAs>
			<MultiProcessorCompilation>true</MultiProcessorCompilation>
		</ClCompile>
		<Link>
			<IgnoreSpecificDefaultLibraries>%(IgnoreSpecificDefaultLibraries)</IgnoreSpecificDefaultLibraries>
			<SubSystem>Console</SubSystem>
			<EntryPointSymbol>WinMainCRTStartup</EntryPointSymbol>
		</Link>
		<ResourceCompile>
			<AdditionalIncludeDirectories>.;D:\github\yuzaos\sample\emulator\scummvm;%(AdditionalIncludeDirectories)</AdditionalIncludeDirectories>
			<PreprocessorDefinitions>USE_ZLIB;USE_MAD;USE_OGG;USE_VORBIS;USE_FLAC;USE_PNG;USE_FREETYPE2;USE_JPEG;USE_FLUIDSYNTH;USE_SCALERS;USE_HQ_SCALERS;USE_RGB_COLOR;USE_HIGHRES;USE_MT32EMU;USE_LUA;USE_NASM;USE_OPENGL;USE_GLES;USE_TASKBAR;USE_CLOUD;USE_TRANSLATION;USE_SYSDIALOGS;USE_DETECTLANG;ENABLE_HUGO;WIN32;SDL_BACKEND;USE_SDL2;%(PreprocessorDefinitions)</PreprocessorDefinitions>
		</ResourceCompile>
	</ItemDefinitionGroup>
</Project>
